{
  "name": "@signumjs/monitor",
  "description": "Monitor transactions on Signum Network blockchain",
  "version": "1.0.0-rc.55",
  "keywords": [
    "signum",
    "signum-network",
    "blockchain",
    "smartcontracts",
    "monitor",
    "signumjs"
  ],
  "contributors": [
    "ohager"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/signum-network/signumjs"
  },
  "engines": {
    "node": ">=10"
  },
  "license": "Apache-2.0",
  "main": "./out/index.js",
  "typings": "./out/index.d.ts",
  "private": false,
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "bili": "^4.10.1",
    "node-localstorage": "^2.1.6",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.24.2"
  },
  "scripts": {
    "test": "jest",
    "tsc": "tsc",
    "bundle": "bili",
    "readme": "jsdoc2md  --files ./src/*.ts --template ./README.md.hbs --configure ../../jsdoc2md.json > ./README.md"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "1388156deae3a6d83b289377ad8e10f86e84dd3c"
}
