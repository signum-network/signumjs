{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
      "outDir": "./out",
      "types": ["axios","vitest/globals"]
    },
  "typeRoots": [
      "typings",
      "node_modules/@types"
    ],
  "include": [
      "./src",
      "README.md"
    ],
  "autogenerate-readme": {
    "srcFiles": [
      "./src/**/*.ts"
    ],
    "template": "./README.md.hbs"
  }
}
