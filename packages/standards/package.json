    "rollup-plugin-typescript2": "^0.24.2"
  },
  "dependencies": {
    "@braintree/sanitize-url": "^6.0.1",
    "@signumjs/core": "^1.0.0-rc.49",
    "better-ajv-errors": "^1.2.0"
  },
  "peerDependencies": {
    "@signumjs/core": "^1.0.0-rc.41"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "gitHead": "1388156deae3a6d83b289377ad8e10f86e84dd3c",
