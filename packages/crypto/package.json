{
  "name": "@signumjs/crypto",
  "version": "3.0.0",
  "description": "Cryptographic functions for building Signum Network apps.",
  "keywords": [
    "signum",
    "signum-network",
    "blockchain",
    "smartcontracts",
    "crypto",
    "cryptography",
    "signumjs"
  ],
  "contributors": [
    "ohager",
    "blankey1337"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/signum-network/signumjs"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "Apache-2.0",
  "sideEffects": false,
  "main": "./out/index.js",
  "exports": {
    ".": {
      "types": "./out/index.d.ts",
      "default": "./out/index.js"
    },
    "./adapters": {
      "types": "./out/adapters/index.d.ts",
      "default": "./out/adapters/index.js"
    },
    "./sha256": {
      "types": "./out/sha256.d.ts",
      "default": "./out/sha256.js"
    },
    "./sign": {
      "types": "./out/sign.d.ts",
      "default": "./out/sign.js"
    },
    "./encryption": {
      "types": "./out/encryption/index.d.ts",
      "default": "./out/encryption/index.js"
    },
    "./random": {
      "types": "./out/random.d.ts",
      "default": "./out/random.js"
    },
    "./mnemonic": {
      "types": "./out/mnemonic.d.ts",
      "default": "./out/mnemonic.js"
    }
  },
  "private": false,
  "devDependencies": {
    "@types/node": "^20.16.0",
    "@types/pako": "^1.0.7",
    "esbuild": "^0.23.1",
    "esbuild-plugin-tsc": "^0.4.0",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4"
  },
  "scripts": {
    "test": "vitest run",
    "test:ci": "vitest run --coverage",
    "compile": "tsc",
    "bundle": "node bundle.js"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "js-sha256": "^0.11.0",
    "pako": "^1.0.11"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "gitHead": "1388156deae3a6d83b289377ad8e10f86e84dd3c",
  "publishConfig": {
    "access": "public"
  }
}
