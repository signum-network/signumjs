{
  "name": "@signumjs/wallets",
  "version": "1.0.0",
  "description": "Wallets communication package for DApps in the Signum Network",
  "keywords": [
    "signum",
    "signum-network",
    "blockchain",
    "smartcontracts",
    "crypto",
    "cryptography",
    "cryptocoin",
    "signumjs",
    "dapps",
    "web3"
  ],
  "contributors": [
    "ohager"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/signum-network/signumjs"
  },
  "engines": {
    "node": ">=10"
  },
  "license": "Apache-2.0",
  "main": "./out/index.js",
  "typings": "./out/index.d.ts",
  "private": false,
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^17.0.8",
    "bili": "^4.10.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.24.2"
  },
  "scripts": {
    "test": "jest",
    "compile": "tsc",
    "bundle": "node bundle.js",
    "readme": "jsdoc2md  --files ./src/*.ts --template ./README.md.hbs --configure ../../jsdoc2md.json > ./README.md"
  },
  "dependencies": {
    "@signumjs/util": "*",
    "uuid": "^10.0.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "gitHead": "1388156deae3a6d83b289377ad8e10f86e84dd3c",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@signumjs/core": "^1.0.0",
    "@signumjs/crypto": "^1.0.0"
  }
}
