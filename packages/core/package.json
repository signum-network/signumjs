{
  "name": "@signumjs/core",
  "version": "1.0.0-rc.41",
  "description": "Principal package with functions and models for building Signum Network applications.",
  "keywords": [
    "signum",
    "signum-network",
    "blockchain",
    "smartcontracts",
    "crypto",
    "cryptography",
    "cryptocoin",
    "signumjs"
  ],
  "contributors": [
    "ohager",
    "blankey1337"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/signum-network/signumjs"
  },
  "engines": {
    "node": ">=10"
  },
  "license": "Apache-2.0",
  "main": "./out/index.js",
  "typings": "./out/index.d.ts",
  "private": false,
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "devDependencies": {
    "@types/bignumber.js": "^5.0.0",
    "@types/jest": "^24.0.12",
    "bili": "^4.10.1",
    "dotenv": "^8.2.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.24.2"
  },
  "scripts": {
    "test": "jest",
    "tsc": "tsc",
    "bundle": "bili",
    "readme": "jsdoc2md  --files ./src/**/*.ts --template ./README.md.hbs --configure ../../jsdoc2md.json > ./README.md"
  },
  "dependencies": {
    "bignumber.js": "^9.0.0",
    "@signumjs/contracts": "^1.0.0-rc.41",
    "@signumjs/crypto": "^1.0.0-rc.41",
    "@signumjs/http": "^1.0.0-rc.41",
    "@signumjs/util": "^1.0.0-rc.41"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "gitHead": "0b88a2e0510f3f0d0a4f868e68129a42f9612ffa",
  "publishConfig": {
    "access": "public"
  }
}
