{
  "name": "@signumjs/core",
  "version": "3.0.0",
  "description": "Principal package with functions and models for building Signum Network applications.",
  "keywords": [
    "signum",
    "signum-network",
    "blockchain",
    "smartcontracts",
    "crypto",
    "cryptography",
    "cryptocoin",
    "signumjs"
  ],
  "contributors": [
    "ohager",
    "blankey1337"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/signum-network/signumjs"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "Apache-2.0",
  "sideEffects": false,
  "main": "./out/index.js",
  "exports": {
    ".": {
      "types": "./out/index.d.ts",
      "default": "./out/index.js"
    },
    "./api/account": {
      "types": "./out/api/factories/index.account.d.ts",
      "default": "./out/api/factories/index.account.js"
    },
    "./api/transaction": {
      "types": "./out/api/factories/index.transaction.d.ts",
      "default": "./out/api/factories/index.transaction.js"
    },
    "./api/message": {
      "types": "./out/api/factories/index.message.d.ts",
      "default": "./out/api/factories/index.message.js"
    },
    "./api/asset": {
      "types": "./out/api/factories/index.asset.d.ts",
      "default": "./out/api/factories/index.asset.js"
    },
    "./api/contract": {
      "types": "./out/api/factories/index.contract.d.ts",
      "default": "./out/api/factories/index.contract.js"
    },
    "./api/alias": {
      "types": "./out/api/factories/index.alias.d.ts",
      "default": "./out/api/factories/index.alias.js"
    },
    "./api/block": {
      "types": "./out/api/factories/index.block.d.ts",
      "default": "./out/api/factories/index.block.js"
    },
    "./api/network": {
      "types": "./out/api/factories/index.network.d.ts",
      "default": "./out/api/factories/index.network.js"
    },
    "./createReadOnlyClient": {
      "types": "./out/ledgerClient/createReadOnlyClient.d.ts",
      "default": "./out/ledgerClient/createReadOnlyClient.js"
    },
    "./createClient": {
      "types": "./out/ledgerClient/createClient.d.ts",
      "default": "./out/ledgerClient/createClient.js"
    },
    "./createClientWithEncryptedMessaging": {
      "types": "./out/ledgerClient/createClientWithEncryptedMessaging.d.ts",
      "default": "./out/ledgerClient/createClientWithEncryptedMessaging.js"
    }
  },
  "private": false,
  "devDependencies": {
    "@types/bignumber.js": "^5.0.0",
    "dotenv": "^8.2.0",
    "esbuild": "^0.23.1",
    "esbuild-plugin-tsc": "^0.4.0",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4"
  },
  "scripts": {
    "test": "vitest run",
    "test:ci": "vitest run --coverage",
    "compile": "tsc",
    "bundle": "node bundle.js"
  },
  "dependencies": {
    "@signumjs/contracts": "*",
    "@signumjs/crypto": "*",
    "@signumjs/http": "*",
    "@signumjs/util": "*",
    "bignumber.js": "^9.1.2"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "gitHead": "1388156deae3a6d83b289377ad8e10f86e84dd3c",
  "publishConfig": {
    "access": "public"
  }
}
